#. module: ptplus_stock
#: model:mail.template,body_html:ptplus_stock.email_template_stock_deliveryslip
msgid ""
"<div style=\"margin: 0px; padding: 0px;\">\n"
"    <p style=\"margin: 0px; padding: 0px; font-size: 13px;\">\n"
"        Dear ${object.pt_get_partner('to', 'nominal').name},\n"
"        <br/><br/>\n"
"        Please find attached document <strong>${object.pt_get_fullname() or "
"'n/a' }</strong>,\n"
"        dated ${format_date(object.date_issued)}.\n"
"        <br/><br/>\n"
"        Do not hesitate to contact us if you have any question.\n"
"        <br/><br/>\n"
"        Regards,\n"
"    </p>\n"
"</div>\n"
"            "
msgstr ""
"<div style=\"margin: 0px; padding: 0px;\">\n"
"    <p style=\"margin: 0px; padding: 0px; font-size: 13px;\">\n"
"        Caro(a) ${object.pt_get_partner('to', 'nominal').name},\n"
"        <br/><br/>\n"
"        Junto enviamos o documento <strong>${object.pt_get_fullname() or 'n/"
"a' }</strong>,\n"
"        de ${format_date(object.date_issued)}.\n"
"        <br/><br/>\n"
"        Caso haja alguma questão, não hesite em contactar-nos.\n"
"        <br/><br/>\n"
"        Cumprimentos,\n"
"    </p>\n"
"</div>\n"
"            "

